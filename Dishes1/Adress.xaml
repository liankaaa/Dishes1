                <ImageBrush ImageSource="/korzina.png" Stretch="None"/>
            </Button.Background>
        </Button>
        <Button Content="" HorizontalAlignment="Left" Height="45" Margin="612,37,0,0" VerticalAlignment="Top" Width="55" BorderBrush="White" Click="Button_Click_2">
            <Button.Background>
                <ImageBrush ImageSource="/adress.png" Stretch="None"/>
            </Button.Background>
        </Button>
        <Rectangle HorizontalAlignment="Left" Height="252" Margin="77,153,0,0" Stroke="White" VerticalAlignment="Top" Width="209">
            <Rectangle.Fill>
                <ImageBrush ImageSource="/Rectangle 34.png" Stretch="Uniform"/>
            </Rectangle.Fill>
        </Rectangle>
        <Label Content="Пункт выдачи" HorizontalAlignment="Left" Margin="122,165,0,0" VerticalAlignment="Top" FontFamily="Arial" FontSize="16"/>
        <Label Content="ул. Комсольмольская, 36" HorizontalAlignment="Left" Margin="90,201,0,0" VerticalAlignment="Top"/>
        <Label Content="ул. Комсольмольская, 36" HorizontalAlignment="Left" Margin="90,232,0,0" VerticalAlignment="Top"/>
        <Label Content="ул. Комсольмольская, 36" HorizontalAlignment="Left" Margin="89,263,0,0" VerticalAlignment="Top"/>
        <Label Content="ул. Комсольмольская, 36" HorizontalAlignment="Left" Margin="89,294,0,0" VerticalAlignment="Top"/>
        <Label Content="ул. Комсольмольская, 36" HorizontalAlignment="Left" Margin="89,325,0,0" VerticalAlignment="Top"/>
        <CheckBox Content="CheckBox" HorizontalAlignment="Left" Height="13" Margin="250,207,0,0" VerticalAlignment="Top" Width="18"/>
        <CheckBox Content="CheckBox" HorizontalAlignment="Left" Height="13" Margin="250,239,0,0" VerticalAlignment="Top" Width="18"/>
        <CheckBox Content="CheckBox" HorizontalAlignment="Left" Height="13" Margin="250,269,0,0" VerticalAlignment="Top" Width="18"/>
        <CheckBox Content="CheckBox" HorizontalAlignment="Left" Height="13" Margin="250,299,0,0" VerticalAlignment="Top" Width="18"/>
        <CheckBox Content="CheckBox" Height="13" Margin="250,331,532,0" VerticalAlignment="Top"/>
        <Rectangle HorizontalAlignment="Left" Height="254" Margin="366,148,0,0" Stroke="White" VerticalAlignment="Top" Width="373">
            <Rectangle.Fill>
                <ImageBrush ImageSource="/Frame 4.png" Stretch="Uniform"/>
            </Rectangle.Fill>
        </Rectangle>

    </Grid>
</Window>
